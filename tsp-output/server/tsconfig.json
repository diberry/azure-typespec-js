{
  "compilerOptions": {
    "target": "es2020",
    "module": "Node16",
    "moduleResolution": "node16",
    "rootDir": "./",
    "outDir": "./dist/",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "sourceMap": true
  },
  "include": [
    "src/**/*.ts" ]
}
